<%= link("new subject", to: subject_path(@conn, :new), class: "btn btn-primary")  %>

<table class="table table-striped" cellspacing="0", width="100%">
  <thead>
    <tr>
      <th></th>
      <th>Last Updated</th>
    </tr>
  </thead>
  <tbody>
    <%= for subject <- @subjects do %>
      <tr>
        <td><%= link(subject.title, to: subject_path(@conn, :show, subject)) %></td>
        <td><%= subject.updated_at %></td>
      </tr>
  <% end %>

  </tbody>
</table>

